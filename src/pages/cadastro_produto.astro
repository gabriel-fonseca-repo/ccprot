---
import Layout from "../layouts/Layout.astro";
---

<Layout title="Cadastre seus produtos">
	<form>
		<div class="d-none d-md-flex flex-row justify-content-evenly align-items-center">
			<img class="rounded" src="data:image/jpeg;base64," alt="Red dot" />
			<div class="card">
				<div class="card-body">
					<h5 class="card-title">Cadastro do produto</h5>
					<div class="mb-3">
						<label for="nome_produto" class="form-label">Nome do produto</label>
						<input type="text" class="form-control" id="nome_produto" />
					</div>
					<div class="mb-3">
						<label for="data_venc_produto" class="form-label">Data do vencimento</label>
						<input type="date" class="form-control" id="data_venc_produto" />
					</div>
					<div class="mb-3">
						<label for="descricao_produto" class="form-label">Descrição do produto</label>
						<textarea class="form-control" id="descricao_produto" rows="3"></textarea>
					</div>
					<div class="mb-3">
						<label for="imagem_produto" class="form-label">Imagem para exibição do produto</label>
						<input class="form-control" type="file" id="imagem_produto" />
					</div>
				</div>
			</div>
		</div>
		<div class="d-md-none">
			<div class="card">
				<img class="card-img-top" src="data:image/jpeg;base64," alt="Red dot" />
				<div class="card-body">
					<h5 class="card-title">Cadastro do produto</h5>
					<div class="mb-3">
						<label for="nome_produto" class="form-label">Nome do produto</label>
						<input type="text" class="form-control" id="nome_produto" />
					</div>
					<div class="mb-3">
						<label for="data_venc_produto" class="form-label">Data do vencimento</label>
						<input type="date" class="form-control" id="data_venc_produto" />
					</div>
					<div class="mb-3">
						<label for="descricao_produto" class="form-label">Descrição do produto</label>
						<textarea class="form-control" id="descricao_produto" rows="3"></textarea>
					</div>
					<div class="mb-3">
						<label for="imagem_produto" class="form-label">Imagem para exibição do produto</label>
						<input class="form-control" type="file" id="imagem_produto" />
					</div>
				</div>
			</div>
		</div>
	</form>
</Layout>
